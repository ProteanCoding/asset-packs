{
  "id": "ce058a68-1635-472c-b4b5-4bc9f4b4277f",
  "name": "Old Wooden Door",
  "category": "structures",
  "tags": [
    "door"
  ],
  "components": {
    "core::GltfContainer": {
      "src": "{assetPath}/door_pirates.glb"
    },
    "asset-packs::Actions": {
      "value": [
        {
          "name": "Open",
          "type": "play_animation",
          "payload": {
            "playAnimation": {
              "animation": "open"
            }
          }
        },
        {
          "name": "Close",
          "type": "play_animation",
          "payload": {
            "playAnimation": {
              "animation": "close"
            }
          }
        }
      ]
    },
    "asset-packs::Triggers": {
      "value": [
        {
          "type": "on_click",
          "actions": [
            {
              "entity": "{selfEntity}",
              "name": "Open"
            }
          ]
        },
        {
          "type": "on_click",
          "actions": [
            {
              "entity": "{selfEntity}",
              "name": "Close"
            }
          ]
        }
      ]
    },
    "asset-packs::States": {
      "value": [
        "Open",
        "Close"
      ],
      "defaultValue": "Close"
    }
  }
}
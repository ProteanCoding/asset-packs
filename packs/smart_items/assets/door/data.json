{
  "id": "a351d9e4-030d-47bd-abca-16103cd8849d",
  "name": "A door",
  "category": "decorations",
  "tags": [],
  "components": {
    "core::GltfContainer": {
      "src": "{assetPath}/Door.glb"
    },
    "asset-packs::Actions": {
      "value": [
        {
          "name": "Open",
          "type": "play_animation",
          "payload": {
            "playAnimation": {
              "animation": "Open"
            }
          }
        },
        {
          "name": "Close",
          "type": "play_animation",
          "payload": {
            "playAnimation": {
              "animation": "Close"
            }
          }
        }
      ]
    },
    "asset-packs::Triggers": {
      "value": [
        {
          "type": "on_click",
          "actions": [
            {
              "entity": "{selfEntity}",
              "name": "Open"
            }
          ]
        }
      ]
    },
    "asset-packs::States": {
      "value": [
        "Open",
        "Close"
      ],
      "defaultValue": "Close"
    }
  }
}
